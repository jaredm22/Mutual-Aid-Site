generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MutualAid {
  id           Int      @id @default(autoincrement())
  name         String
  neighborhood String[]
  phone        String
  email        String
  website      String
  give_help    String
  need_help    String
  address_one  String
  address_two  String
  city         String
  state        String
  zip          String
  tags         String[]
  days         String[]
  times        String[]
}
